{
  "manifest_version": 2,
  "name": "web.music.control",
  "version": "2.0",
  "icons": {
    "48": "play.png"
  },
  "background": {
    "persistent": true,
    "page": "background.html"
  },
  "content_security_policy": "script-src http://localhost:18711; object-src 'self' http://localhost;",
  "permissions": [
    "tabs",
    "http://www.silver.ru/*",
    "http://www.mixcloud.com/*",
    "https://www.mixcloud.com/*",
    "http://new.vk.com/*",
    "https://new.vk.com/*",
    "http://vk.com/*",
    "https://vk.com/*",
    "http://music.yandex.ru/*",
    "https://music.yandex.ru/*",
    "http://8tracks.com/*",
    "https://8tracks.com/*",
    "http://soundcloud.com/*",
    "https://soundcloud.com/*",
    "http://localhost:18711/*",
    "https://play.google.com/*"
  ],
  "browser_action": {
    "default_title": "web.music.control",
    "default_icon": "pause.png",
    "default_popup": "popup.html"
  }
}